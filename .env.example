SIMPaisa_TIMEOUT=30

# Simpaisa Base URLs (Optional - auto-set based on SIMPaisa_MODE)
# Payin (Wallet) Base URL - Production: https://wallets.simpaisa.com, Sandbox: https://sandbox.simpaisa.com
SIMPaisa_BASE_URL_PAYIN=https://sandbox.simpaisa.com
# Payout (Disbursement) Base URL - Production: https://disb.simpaisa.com, Sandbox: https://sandbox.simpaisa.com
SIMPaisa_BASE_URL_PAYOUT=https://sandbox.simpaisa.com

# Simpaisa Default Product Reference (Optional)
SIMPaisa_DEFAULT_PRODUCT_REFERENCE=default-product-reference

# RSA Digital Signature Keys (Paths relative to storage/app or absolute paths)
SIMPaisa_RSA_PRIVATE_KEY_PATH=storage/app/keys/merchant_private_key.pem
SIMPaisa_RSA_PUBLIC_KEY_PATH=storage/app/keys/merchant_public_key.pem
SIMPaisa_RSA_SIMPaisa_PUBLIC_KEY_PATH=storage/app/keys/simpaisa_public_key.pem

# SSL/TLS Certificates (Paths relative to storage/app or absolute paths)
